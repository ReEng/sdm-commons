<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

 <plugin>

  <!-- EEF extensions -->
   <extension point="org.eclipse.emf.edit.itemProviderAdapterFactories">
      <factory
            uri="http://ns.storydriven.org/sdm/activities/expressions/0.1.0"
            class="org.storydriven.modeling.activities.providers.ExpressionsEEFAdapterFactory"
            supportedTypes="org.eclipse.emf.eef.runtime.providers.PropertiesEditingProvider"/>
   </extension>
   <extension point="org.eclipse.emf.edit.itemProviderAdapterFactories">
      <factory
            uri="http://ns.storydriven.org/sdm/activities/0.1.0"
            class="org.storydriven.modeling.activities.providers.ActivitiesEEFAdapterFactory"
            supportedTypes="org.eclipse.emf.eef.runtime.providers.PropertiesEditingProvider"/>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="org.storydriven.modeling.calls.providers.CallsPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="org.storydriven.modeling.calls.expressions.providers.ExpressionsPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="org.storydriven.modeling.templates.providers.TemplatesPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="org.storydriven.modeling.patterns.providers.PatternsPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="org.storydriven.modeling.activities.expressions.providers.ExpressionsPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="org.storydriven.modeling.patterns.expressions.providers.ExpressionsPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="org.storydriven.modeling.activities.providers.ActivitiesPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="org.storydriven.modeling.expressions.providers.ExpressionsPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="org.storydriven.modeling.providers.ModelingPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>

   <!-- Tabbed properties views extension -->
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyContributor">
      <propertyContributor
            contributorId="org.storydriven.modeling.activities.properties">
         <propertyCategory
               category="default">
         </propertyCategory>
         <propertyCategory
               category="extended">
         </propertyCategory>
         <propertyCategory
               category="advanced">
         </propertyCategory>
      </propertyContributor>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyTabs">
      <propertyTabs
            contributorId="org.storydriven.modeling.activities.properties">
         <propertyTab
               label="Base"
               category="default"
               id="Base">
         </propertyTab>
      </propertyTabs>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertySections">
      <propertySections
            contributorId="org.storydriven.modeling.activities.properties">
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.storydriven.modeling.activities.section.ExceptionVariable"
               tab="Base">
             <input
                    type="org.storydriven.modeling.activities.ExceptionVariable">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.storydriven.modeling.activities.section.ActivityEdge"
               tab="Base">
             <input
                    type="org.storydriven.modeling.activities.ActivityEdge">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.storydriven.modeling.activities.section.Activity"
               tab="Base">
             <input
                    type="org.storydriven.modeling.activities.Activity">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.storydriven.modeling.activities.section.OperationExtension"
               tab="Base">
             <input
                    type="org.storydriven.modeling.activities.OperationExtension">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.storydriven.modeling.activities.section.MatchingStoryNode"
               tab="Base">
             <input
                    type="org.storydriven.modeling.activities.MatchingStoryNode">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.storydriven.modeling.activities.section.StructuredNode"
               tab="Base">
             <input
                    type="org.storydriven.modeling.activities.StructuredNode">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.storydriven.modeling.activities.section.JunctionNode"
               tab="Base">
             <input
                    type="org.storydriven.modeling.activities.JunctionNode">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.storydriven.modeling.activities.section.StartNode"
               tab="Base">
             <input
                    type="org.storydriven.modeling.activities.StartNode">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.storydriven.modeling.activities.section.StatementNode"
               tab="Base">
             <input
                    type="org.storydriven.modeling.activities.StatementNode">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.storydriven.modeling.activities.section.StopNode"
               tab="Base">
             <input
                    type="org.storydriven.modeling.activities.StopNode">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.storydriven.modeling.activities.section.ActivityCallNode"
               tab="Base">
             <input
                    type="org.storydriven.modeling.activities.ActivityCallNode">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.storydriven.modeling.activities.section.ModifyingStoryNode"
               tab="Base">
             <input
                    type="org.storydriven.modeling.activities.ModifyingStoryNode">
             </input>
         </propertySection>
      </propertySections>
   </extension>
 </plugin>
