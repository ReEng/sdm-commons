<?xml version="1.0" encoding="UTF-8"?>
<sda:Activity xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:sda="http://www.storydriven.org/storydiagrams/activities/0.2.0" xmlns:sdce="http://www.storydriven.org/storydiagrams/calls/expressions/0.2.0" xmlns:expr="http://www.storydriven.org/core/expressions/0.2.0" xmlns:sdp="http://www.storydriven.org/storydiagrams/patterns/0.2.0">
  <ownedActivityEdge target="//@ownedActivityNode.2" source="//@ownedActivityNode.0"/>
  <ownedActivityEdge target="//@ownedActivityNode.1" source="//@ownedActivityNode.2"/>
  <ownedActivityNode xsi:type="sda:StartNode" outgoing="//@ownedActivityEdge.0"/>
  <ownedActivityNode xsi:type="sda:StopNode" incoming="//@ownedActivityEdge.1">
