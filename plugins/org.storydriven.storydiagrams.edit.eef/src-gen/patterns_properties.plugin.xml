<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

 <plugin>

  <!-- EEF extensions -->
   <extension point="org.eclipse.emf.edit.itemProviderAdapterFactories">
      <factory
            uri="http://www.storydriven.org/storydiagrams/patterns/0.2.0"
            class="org.storydriven.storydiagrams.patterns.providers.PatternsEEFAdapterFactory"
            supportedTypes="org.eclipse.emf.eef.runtime.providers.PropertiesEditingProvider"/>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="org.storydriven.storydiagrams.calls.providers.CallsPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="org.storydriven.storydiagrams.patterns.expressions.providers.ExpressionsPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="org.storydriven.storydiagrams.providers.StorydiagramsPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="org.storydriven.storydiagrams.templates.providers.TemplatesPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="org.storydriven.storydiagrams.patterns.providers.PatternsPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="org.storydriven.storydiagrams.calls.expressions.providers.ExpressionsPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="org.storydriven.storydiagrams.activities.expressions.providers.ExpressionsPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="org.storydriven.storydiagrams.activities.providers.ActivitiesPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>

   <!-- Tabbed properties views extension -->
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyContributor">
      <propertyContributor
            contributorId="org.storydriven.storydiagrams.patterns.properties">
         <propertyCategory
               category="default">
         </propertyCategory>
         <propertyCategory
               category="extended">
         </propertyCategory>
         <propertyCategory
               category="advanced">
         </propertyCategory>
      </propertyContributor>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyTabs">
      <propertyTabs
            contributorId="org.storydriven.storydiagrams.patterns.properties">
         <propertyTab
               label="Base"
               category="default"
               id="Base">
         </propertyTab>
      </propertyTabs>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertySections">
      <propertySections
            contributorId="org.storydriven.storydiagrams.patterns.properties">
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.storydriven.storydiagrams.patterns.section.ObjectVariable"
               tab="Base">
             <input
                    type="org.storydriven.storydiagrams.patterns.ObjectVariable">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.storydriven.storydiagrams.patterns.section.Constraint"
               tab="Base">
             <input
                    type="org.storydriven.storydiagrams.patterns.Constraint">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.storydriven.storydiagrams.patterns.section.LinkConstraint"
               tab="Base">
             <input
                    type="org.storydriven.storydiagrams.patterns.LinkConstraint">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.storydriven.storydiagrams.patterns.section.AttributeAssignment"
               tab="Base">
             <input
                    type="org.storydriven.storydiagrams.patterns.AttributeAssignment">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.storydriven.storydiagrams.patterns.section.ObjectSetVariable"
               tab="Base">
             <input
                    type="org.storydriven.storydiagrams.patterns.ObjectSetVariable">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.storydriven.storydiagrams.patterns.section.PrimitiveVariable"
               tab="Base">
             <input
                    type="org.storydriven.storydiagrams.patterns.PrimitiveVariable">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.storydriven.storydiagrams.patterns.section.Path"
               tab="Base">
             <input
                    type="org.storydriven.storydiagrams.patterns.Path">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.storydriven.storydiagrams.patterns.section.LinkVariable"
               tab="Base">
             <input
                    type="org.storydriven.storydiagrams.patterns.LinkVariable">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.storydriven.storydiagrams.patterns.section.ContainmentRelation"
               tab="Base">
             <input
                    type="org.storydriven.storydiagrams.patterns.ContainmentRelation">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.storydriven.storydiagrams.patterns.section.MatchingPattern"
               tab="Base">
             <input
                    type="org.storydriven.storydiagrams.patterns.MatchingPattern">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.storydriven.storydiagrams.patterns.section.ContainerVariable"
               tab="Base">
             <input
                    type="org.storydriven.storydiagrams.patterns.ContainerVariable">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="org.storydriven.storydiagrams.patterns.section.StoryPattern"
               tab="Base">
             <input
                    type="org.storydriven.storydiagrams.patterns.StoryPattern">
             </input>
         </propertySection>
      </propertySections>
   </extension>
 </plugin>
