<feature
      id="org.storydriven.storydiagrams.interpreter.feature"
      label="Story Diagram Tooling: Interpreter"
      version="1.1.0.qualifier"
      provider-name="storydriven.org">

   <description url="http://www.storydriven.org">
      This feature contains the specific plug-ins to integrate the story diagram model into the core interpreter.
   </description>

   <requires>
      <import plugin="org.storydriven.storydiagrams.interpreter.core" version="1.0.1" match="greaterOrEqual"/>
      <import plugin="de.mdelab.sdm.interpreter.core.eclipse" version="1.0.0" match="greaterOrEqual"/>
      <import feature="org.storydriven.core.feature" version="1.0.1.qualifier"/>
      <import feature="org.storydriven.storydiagrams.feature" version="1.0.1.qualifier"/>
      <import plugin="de.mdelab.sdm.interpreter.core" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.storydriven.core" version="1.0.1" match="greaterOrEqual"/>
      <import plugin="org.storydriven.storydiagrams" version="1.0.1" match="greaterOrEqual"/>
      <import plugin="org.storydriven.storydiagrams.expressions"/>
      <import plugin="org.eclipse.emf.ecore"/>
   </requires>

   <plugin
         id="org.storydriven.storydiagrams.interpreter.core"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.storydriven.storydiagrams.interpreter.eclipse"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
